{
  "name": "clogit",
  "version": "0.0.1",
  "description": "Javascript logger",
  "main": "dist/clogit.js",
  "browser": "dist/clogit.min.js",
  "module": "dist/clogit.esm.js",
  "types": "dist/index.d.ts",
  "author": "jccit",
  "license": "MIT",
  "scripts": {
    "build": "concurrently \"yarn run build:types\" \"yarn run build:bundle && yarn run build:browser\" \"yarn run docs\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "rollup -c rollup.config.js",
    "build:browser": "rollup -c rollup.min.config.js",
    "dev": "rollup -c rollup.config.js -w",
    "docs": "typedoc --out docs/api src",
    "type-check": "tsc --noEmit",
    "test": "yarn run type-check && yarn run jest",
    "jest": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "concurrently": "^4.1.0",
    "jest": "^24.7.1",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "tslint": "^5.15.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1"
  }
}
